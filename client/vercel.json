{
  "version": 2,
  "routes": [
    {
      "src": "/BuildDb/(.*)",
      "dest": "/BuildDb/$1",
      "headers": { "Cache-Control": "public, max-age=0, must-revalidate" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }]
}
